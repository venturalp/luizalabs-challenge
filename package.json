{
  "name": "venturalp-luizalabs-challenge",
  "version": "1.0.0",
  "description": "Luizalabs Challenge - venturalp",
  "main": "webpack.config.js",
  "scripts": {
    "build:dev": "webpack --mode development --progress --inline --color",
    "start:prd": "webpack --mode production --progress --inline --color && webpack-dev-server --mode production --open",
    "build": "webpack --mode production --progress --inline --color",
    "start": "webpack --mode development --progress --inline --color && webpack-dev-server --mode development --open",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "analyze": "source-map-explorer dist/js/*.js --gzip --html source-map-explorer.html",
    "lint-fix": "eslint --ext .jsx,.js src --fix --quiet",
    "lint": "eslint src/**/*.{js,jsx}",
    "test": "jest -o --maxWorkers=4",
    "test:all": "jest --maxWorkers=4",
    "test:update": "jest -o -u --maxWorkers=4",
    "test:update:all": "jest -u --maxWorkers=4",
    "coverage": "jest --coverage --maxWorkers=4 && istanbul-badges-readme",
    "cz": "git-cz",
    "make-badges": "istanbul-badges-readme"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/venturalp/luizalabs-challenge.git"
  },
  "author": "Guilherme Ventura de Souza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/venturalp/luizalabs-challenge/issues"
  },
  "homepage": "https://github.com/venturalp/luizalabs-challenge",
  "config": {
    "commitizen": {
      "path": "commitiquette"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "axios": "^0.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-loading": "^2.0.3",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.2.1",
    "zustand": "^2.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.6",
    "@babel/register": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@svgr/webpack": "^5.3.0",
    "@testing-library/dom": "^7.20.1",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^12.0.11",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitiquette": "^1.0.3",
    "commitizen": "^4.1.2",
    "conventional-changelog-cli": "^2.0.34",
    "css-loader": "^5.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.7",
    "eslint-plugin-unused-imports": "^0.1.2",
    "file-loader": "^6.0.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^4.2.3",
    "istanbul-badges-readme": "^1.0.5",
    "jest": "^26.1.0",
    "msw": "^0.21.3",
    "path": "^0.12.7",
    "prettier": "^2.0.2",
    "react-test-renderer": "^16.13.1",
    "source-map-explorer": "^2.4.1",
    "source-map-loader": "^1.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
